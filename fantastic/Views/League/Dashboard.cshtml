
@{
    ViewData["Title"] = "New League Registration";
}

@using fantastic.Models;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model fantastic.Models.NewLeague

<form asp-controller="League" asp-action="create" name="create" method="POST">
    <h5>
        <label asp-for="Name"></label>
        <input asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </h5>
    <h5>
        <label asp-for="Start" ></label>
        <input asp-for="Start" id="datePicker1"/>
        <span asp-validation-for="Start"></span>
    </h5>
    <h5>
        <label asp-for="End"></label>
        <input asp-for="End" id="datePicker2"/>
        <span asp-validation-for="End"></span>
    </h5>
    <h5>
        <label asp-for="Duration"></label>
        <input asp-for="Duration" />
        <span asp-validation-for="Duration"></span>
    </h5>
    <button type="submit">Create League</button>
</form>